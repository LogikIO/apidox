<?xml version="1.0" encoding="UTF-8"?>

<method type="GET" uri="urlshortener/v1/url"
	description="You can call this method to expand any goo.gl short URL."
	status="PRODUCTION">

	<param name="shortUrl" type="string" description="Is the short URL to expand."
		required="N" defaultValue="http://goo.gl/fbsS" />

<errorcodes>
<error code="300" />
</errorcodes>

	<response>
		<success>
<![CDATA[
{
 "kind": "urlshortener#url",
 "id": "http://goo.gl/fbsS",
 "longUrl": "http://www.google.com/",
 "status": "OK"
}
]]></success>
	</response>

</method>

